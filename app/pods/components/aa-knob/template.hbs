<div class="title">
  {{title}}
</div>

<div class="label-mid">{{mid}}</div>

<div class="knob style">
  <div class="rela-block knob-dial">
    <div class="abs-center dial-grip" style="{{dialStyle}}" />
    <svg viewBox="0 0 100 100" class="dial-svg">
      <path d="M20,76 A 40 40 0 1 1 80 76" fill="none" class="inner-circle" />
      <path d="M20,76 A 40 40 0 1 1 80 76" fill="none" class="outer-circle" style="{{outerStyle}}" />
    </svg>
  </div>
</div>

<div class="bottom-label-container">
  <div class="label min">{{min}}</div>
  <div class="label max">{{max}}</div>
</div>
